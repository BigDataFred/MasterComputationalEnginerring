# 3 Registros de desplazamiento (LSFRs)
LFSR <- function(cD,s0,n){
s <- s0
L <-length(s0)
out <- NULL
out <- rbind(out,s0)
cIx <- which(cD==1)
k<-2
j<-length(s)+1
while (TRUE){
tmp<-vector(length=L)
tmp[1]<- sum(out[k-1,cIx])%%2
tmp[2:L]<-out[k-1,1:L-1]
out <- rbind(out,tmp)
s <- c(s,sum(s[j-cIx])%%2)
if (length(s) ==n){ # all(out[k,] ==s0)
return(s[1:n])
}
k<-k+1
j<-j+1
}
}
